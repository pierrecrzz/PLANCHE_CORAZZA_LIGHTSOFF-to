public class Cellule {
    boolean Active;
    int X;
    int Y;
    boolean victoire = false;
    
    public Cellule ( int X; int Y){
        X=x;
        Y=y;
    }
    
   public voic activate(){
        Active= True;
   }
   
   public void desactivate () {
        Active =False ; 
   }
}
